<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bugsnag.android.mazerunner">

    <application
        android:label="MazeRunner">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity">
            <intent-filter>
                <action android:name="com.bugsnag.android.mazerunner.UPDATE_CONTEXT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <meta-data android:name="com.bugsnag.android.API_KEY" android:value="ABCDEFGHIJKLMNOPQRSTUVWXYZ012345"/>
        <meta-data android:name="com.bugsnag.android.ENDPOINT" android:value="http://bs-local.com:9339"/>
        <meta-data android:name="com.bugsnag.android.SESSIONS_ENDPOINT" android:value="http://bs-local.com:9339"/>
        <meta-data android:name="com.bugsnag.android.AUTO_CAPTURE_SESSIONS" android:value="true"/>
        <meta-data android:name="com.bugsnag.android.APP_VERSION" android:value="9.8.7"/>
        <meta-data android:name="com.bugsnag.android.RELEASE_STAGE" android:value="testing"/>
        <meta-data android:name="com.bugsnag.android.ENABLED_RELEASE_STAGES" android:value="testing"/>
        <meta-data android:name="com.bugsnag.android.IGNORE_CLASSES" android:value="com.bugsnag.android.mazerunner.IgnorableException"/>
        <meta-data android:name="com.bugsnag.android.REDACT_KEYS" android:value="redacted"/>
    </application>

</manifest>
